<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/screen_home_navigation_fragment_menu_layout"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".HomeActivity">


    <ImageView
        android:id="@+id/screen_home_fragment_menu_imageView_ac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/screen_home_fragment_menu_imageView_bg"
        android:background="@color/background_floating_material_light"
        android:src="@drawable/ic_person_black_48dp" />

    <ImageView
        android:id="@+id/screen_home_fragment_menu_imageView_bg"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_above="@+id/screen_home_fragment_menu_listView"
        android:scaleType="centerCrop"
        android:src="@color/background_floating_material_light" />

    <ListView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/screen_home_fragment_menu_listView"
        android:layout_width="226dp"
        android:layout_height="match_parent"
        android:layout_below="@+id/screen_home_fragment_menu_imageView_bg"
        android:layout_gravity="start"
        android:background="@color/material_blue_grey_900"
        android:dividerHeight="2dp"
        tools:context=".NavigationFragment" />
</LinearLayout><!-- From: file:/C:/Users/Administrator/StudioWorkspace/rcctrl/app/src/main/res/layout/screen_home_navigation_fragment_menu.xml -->